k = [5.184, 2.134, 1.649, 1.296, 1.251, 1.037, 0.955, 0.9072, 0.844, 0.844, 0.825, 0.789, 0.756];
t = [31.25, 31.75, 32, 32.25, 33, 33, 33.5, 34, 34.25, 34.5, 35, 35, 35.5];
p = polyfit(k,t,2);
scatter(k,t,'r*');
y1 = p(2)*k + p(1)*k.^2 + p(3);
hold on;
plot(k, y1, '-b');
title('T v/s K')
xlabel('K')
ylabel('T')
grid on
